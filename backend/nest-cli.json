{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/backend/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/backend/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/backend",
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "backend": {
      "type": "application",
      "root": "apps/backend",
      "entryFile": "main",
      "sourceRoot": "apps/backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/backend/tsconfig.app.json"
      }
    },
    "meme": {
      "type": "application",
      "root": "apps/meme",
      "entryFile": "main",
      "sourceRoot": "apps/meme/src",
      "compilerOptions": {
        "tsConfigPath": "apps/meme/tsconfig.app.json"
      }
    },
    "memes": {
      "type": "application",
      "root": "apps/memes",
      "entryFile": "main",
      "sourceRoot": "apps/memes/src",
      "compilerOptions": {
        "tsConfigPath": "apps/memes/tsconfig.app.json"
      }
    },
    "memes_mirror": {
      "type": "application",
      "root": "apps/memes_mirror",
      "entryFile": "main",
      "sourceRoot": "apps/memes_mirror/src",
      "compilerOptions": {
        "tsConfigPath": "apps/memes_mirror/tsconfig.app.json"
      }
    },
    "plantillas": {
      "type": "application",
      "root": "apps/plantillas",
      "entryFile": "main",
      "sourceRoot": "apps/plantillas/src",
      "compilerOptions": {
        "tsConfigPath": "apps/plantillas/tsconfig.app.json"
      }
    },
    "plantillas_mirror": {
      "type": "application",
      "root": "apps/plantillas_mirror",
      "entryFile": "main",
      "sourceRoot": "apps/plantillas_mirror/src",
      "compilerOptions": {
        "tsConfigPath": "apps/plantillas_mirror/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    }
  }
}